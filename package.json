{
  "name": "epartogram",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "npx tsc",
    "build:watch": "npx tsc --watch",
    "start": "FIREBASE_AUTH_EMULATOR_HOST=\"127.0.0.1:9099\" FIRESTORE_EMULATOR_HOST=\"127.0.0.1:8088\" FIREBASE_DATABASE_EMULATOR_HOST=\"127.0.0.1:9009\" FIREBASE_STORAGE_EMULATOR_HOST=\"127.0.0.1:9199\" GCLOUD_PROJECT=\"e-partogram\" concurrently \"npx tsc --watch\" \"nodemon -q ./dist/src/index.js\"",
    "prod": "node ./dist/src/index.js",
    "prepare": "husky install"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.8.13",
    "@grpc/proto-loader": "^0.7.6",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^16.0.3",
    "express": "~4.16.1",
    "firebase-admin": "^11.5.0",
    "firebase-functions": "^4.2.1",
    "google-protobuf": "^3.21.2",
    "morgan": "~1.9.1"
  },
  "devDependencies": {
    "@bufbuild/buf": "^1.18.0-1",
    "@bufbuild/protobuf": "^1.2.0",
    "@protobuf-ts/plugin": "^2.9.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.17",
    "@types/google-protobuf": "^3.15.6",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.1.2",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "concurrently": "^8.0.1",
    "eslint": "^8.40.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "nodemon": "^2.0.22",
    "prettier": "2.8.8",
    "typescript": "^5.0.4"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown",
    "*.ts": "eslint --fix"
  }
}
